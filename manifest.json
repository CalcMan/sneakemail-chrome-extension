{
  "name": "Sneakemail Filler",
  "version": "0.5",
  "description": "Finds or generates a sneakemail address for an email field, via context menu",
  "permissions": [
    "contextMenus",
    "tabs", "http://*/", "https://*/"
  ],
  "background": {
    "page": "background.html"
  },
  "page_action": {
    "default_name": "Sneakemail Login Info",
    "default_icon": "icon16.png",
    "default_popup": "login-popup.html"
  },
  "manifest_version": 2
}
